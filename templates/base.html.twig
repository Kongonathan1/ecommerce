            crossorigin="anonymous"></script>
        {% endblock %}
    </head>
    <body>
        <style>
            .pagination a {
                padding: 4px 12px;
                border-radius: 2px;
                margin-right: 12px;
                color: rgba(240, 255, 255, 0.338);
                background-color: rgb(34, 93, 221);
                text-decoration: none;
                transition: all .4s;
            }
            .pagination a:hover {
                background-color: rgba(146, 146, 146, 0.768);
                color: rgb(34, 93, 221);
            }
            .pagination .current {
                padding: 4px 12px;
                border-radius: 2px;
                margin-top: -3.5px;
                margin-right: 12px;
                color: rgb(34, 93, 221);
                background-color: rgba(145, 145, 145, 0.748);
            }
            .pagination-container {
                margin: 25px 0;
                margin-left: 60%;
            }
            .y_center {
                padding-top: 15%;
            }
            .bg-light-grey {
                background-color: antiquewhite;
            }
        </style>

        {% include "_partials/_navbar.html.twig" %}

        {% block body %}{% endblock %}
    </body>
</html>

